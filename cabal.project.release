packages: Cabal/
packages: cabal-install/
tests: False
benchmarks: False
optimization: True

optional-packages: ./vendored/*/*.cabal

package Cabal
  ghc-options: -fexpose-all-unfoldings -fspecialise-aggressively

package parsec
  ghc-options: -fexpose-all-unfoldings

constraints: network < 3.1.2.3
